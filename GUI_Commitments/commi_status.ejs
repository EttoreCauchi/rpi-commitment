<body>
	<div class="header">
		<div class="container">
			<nav class="navbar navbar-inverse" role="navigation">
				<div id="main-nav" class="collapse navbar-collapse">
					<ul class="nav navbar-nav">
						<li><a href="home"><font color="black">Home</font></a></li>
						<li><a href="upload_commitment"><font color="black">Upload</font></a></li>
						<li><a href="insert_detail"><font color="black">Check Commitment Status</font></a></li>
						<li><a href="sensor_status"><font color="black">Check Sensors Status</font></a></li>
					</ul>
				</div>
			</nav>
		</div>
	</div>

    <div class="parallax-content baner-content" id="home">
        <div class="container">
            <div class="text-content">
                <h2> <span>COMMITMENT   </span>  <em>   MONITORING</em></h2>

            </div>
        <br>
		<div class="twoColumns">
			<br><br>
			<div>
			<h2>Commitment ID:</h2>	
			<p><font size=5><%= identifier %></font></p>	
			<h2>Commitment Status:</h2>	
			<p><font size=5><%= status %></font></p>
			<h2>Commitment Strength:</h2>
			<p><font size=5><%= strength %></font></p>
			<h2>Commitment Type:</h2>
			<p><font size=5><%= type %></font></p>
			</div>
		</div>
			<div>
			<img src=<%=photo%>>
			</div>
			<div class="clearer"> </div>
		
	</div>
	<br><br><br>
		<form id="contact" action="commitment_status/" method="post">
            <div class="row">
                    <button type="submit" id="form-submit" name="button" value="cancel" class="btn">Cancel</button> &nbsp &nbsp &nbsp
                    <button type="submit" id="form-submit" name="button" value="release" class="btn">Release</button> &nbsp &nbsp &nbsp
                    <button type="submit" id="form-submit" name="button" value="suspend" class="btn">Suspend</button> &nbsp &nbsp &nbsp
                    <button type="submit" id="form-submit" name="button" value="reactivate" class="btn">Reactivate</button>
            </div>
        </form>
	 <br><br>
		<form id="start" action="commitment_status/" method="post">
            <div class="row">
                    <button type="submit" id="form-submit" name="button" value="start" class="btn">Start</button>
            </div>
        </form>	
	
    </div>

		

